scriptmode +i
trapsize 0
setwindow pcb
generaledit
echo Starting mfg process...
echo -----------------------------------------
echo Process Update Shapes...
echo -----------------------------------------
tbx postproc shapeupdate
setwindow pcb
echo -----------------------------------------
echo Process Custom Variables...
echo -----------------------------------------
tbx customvar batch all
setwindow pcb
echo -----------------------------------------
echo Process Cross Section...
echo -----------------------------------------
tbx xsecgen batch
setwindow pcb
echo -----------------------------------------
echo Process Clean Data...
echo -----------------------------------------
tbx postproc cleanup ./mfg/*.*
setwindow pcb
echo -----------------------------------------
echo Process Standard BOM...
echo -----------------------------------------
tbx postproc basicbom
setwindow pcb
echo -----------------------------------------
echo Process Variant BOM...
echo -----------------------------------------
tbx varibom batch core
setwindow pcb
echo -----------------------------------------
echo Process Variant Assembly...
echo -----------------------------------------
tbx variassy batch core
setwindow pcb
echo -----------------------------------------
echo Process NC Drill Legend...
echo -----------------------------------------
ncdrill legend
setwindow form.nc_drilllegend
FORM nc_drilllegend done
setwindow pcb
pick grid 1.000000  430.800000
setwindow pcb
echo -----------------------------------------
echo Process NC Drill Data...
echo -----------------------------------------
nctape_full 
setwindow form.nc_drill
FORM nc_drill execute  
FORM nc_drill close  
setwindow pcb
echo -----------------------------------------
echo Process NC Route Data...
echo -----------------------------------------
ncroute 
setwindow form.nc_route
FORM nc_route execute 
FORM nc_route close 
setwindow pcb
echo -----------------------------------------
echo Process Artwork Data...
echo -----------------------------------------
artwork 
setwindow form.film_control
FORM film_control select_all  
FORM film_control create  
FORM film_control ok  
setwindow pcb
echo -----------------------------------------
echo Process PDF Export...
echo -----------------------------------------
pdf out 
setwindow form.pdf_out
FORM pdf_out filename pultonpcbtest_artwork
FORM pdf_out selectall  
FORM pdf_out doc_open NO 
FORM pdf_out export  
FORM pdf_out cancel  
setwindow pcb
echo -----------------------------------------
echo Process Batch Plot (Legacy)...
echo -----------------------------------------
tbx batchplot plotset all
setwindow pcb
echo -----------------------------------------
echo Process Batch Plot PDF...
echo -----------------------------------------
tbx batchplot pdf all
setwindow pcb
echo -----------------------------------------
echo Process Mfg Collector...
echo -----------------------------------------
tbx mfgcollect batch
setwindow pcb
echo -----------------------------------------
echo Process Zip Data...
echo -----------------------------------------
tbx postproc zipdata output.zip ./mfg
setwindow pcb
